# Frontend Environment Configuration
# Copy this file to .env and customize as needed

# Backend API URL (defaults to localhost:8000)
VITE_API_BASE_URL=http://localhost:8000

# WebSocket URL (defaults to same as API with ws:// protocol)
# VITE_WS_BASE_URL=ws://localhost:8000

# Health check interval in milliseconds (default: 10000)
VITE_HEALTH_CHECK_INTERVAL=10000

# Default lattice size (default: 32)
VITE_DEFAULT_LATTICE_SIZE=32

# Default temperature - critical temperature (default: 2.27)
VITE_DEFAULT_TEMPERATURE=2.27

# Maximum lattice size allowed (default: 128)
VITE_MAX_LATTICE_SIZE=128
